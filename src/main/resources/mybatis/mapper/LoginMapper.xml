<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="project.forums.mapper.LoginMapper">
    <select id="signIn" parameterType="map" resultType="project.forums.domain.Member">
        select member_no, member_id
        from member
        where member_id=#{memberId} and member_pwd=#{memberPwd}
    </select>
</mapper>